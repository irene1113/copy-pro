{
  "name": "copy-pro",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "copy-pro",
    "manifest": "src/manifest.json",
    "main": "copy-pro.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch",
    "resources": [
      "resources/webview.js"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.4",
    "@skpm/extract-loader": "^2.0.2",
    "css-loader": "^3.2.0",
    "html-loader": "^0.5.5",
    "react": "^17.0.1",
    "react-desktop": "^0.3.9",
    "react-dom": "^17.0.1",
    "style-loader": "^1.1.3"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "dependencies": {
    "sketch-module-web-view": "^3.1.4"
  },
  "author": "irene <irene277299@gmail.com>"
}
